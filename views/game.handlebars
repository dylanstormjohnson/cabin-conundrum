<div class='gamePlayContainer container-fluid'>
  <div id="row">
    <div class='roomContainer col-lg-9'>
      {{#each room as |room|}}
      <p>
        {{room.id}}
      </p>
      {{/each}}
    </div>
    <aside class='inventoryContainer float-right col-lg-3'>
      <ul>
        Available Items
        <li>
          {{inventory.id}}
          {{item_name}}
        </li>
        {{!-- add an item selector? --}}
      </ul>
    </aside>
  </div>
</div>